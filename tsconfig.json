{
  "compilerOptions": {
    "target": "es5",
    "strictNullChecks": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  }
}
